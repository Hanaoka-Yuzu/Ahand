}
 
/* 遮罩层 */
 
.mask {
  /* display: block; */
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9000;
  opacity: 0.5;
}
 
/* 弹出层 */
 
.modalDlg {
  width: 400rpx;
  position: fixed;
  top: 25vh;
  left: 0;
  right: 0;
  z-index: 9999;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 5px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
 
