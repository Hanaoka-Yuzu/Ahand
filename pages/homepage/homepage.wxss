}
.movable-area{
  pointer-events:none;
  z-index:100;
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
}


.add_icon{
  position: fixed; 
  width:42px; 
  height:42px;
  bottom: 30px;   
  right: 20px;
}  
.a{ 
  display: flex;
  position: relative;
}
.b{
